package com.yada.spos.front.acq

import org.apache.commons.codec.binary.Hex

/**
  * 收单测试
  */
object AcqClientApp extends App {
  val s = ""
  val bytes = Hex.decodeHex(s.toCharArray)
  val acqClient = new AcqClient()
  val resp = acqClient.send("22.7.24.158", 1000, bytes)
  val respHex = Hex.encodeHexString(resp)
  println(respHex)
}




